<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gabay.workflow.mapper.StaffInfoMapper">
    <select id="selectStaffInfoByStaffNo" resultType="StaffInfo">
        select * from staff_info where staff_no = #{staffNo}
    </select>
</mapper>